<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medicinefinderpharmacy">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/finallogo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.View_med_photo"></activity>
        <activity android:name=".Activity.View_prescription" />
        <activity
            android:name=".Activity.Preorder"
            android:label="@string/title_activity_preorder"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity
            android:name=".Activity.Order_detail"
            android:label="@string/title_activity_order_detail"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity
            android:name=".Activity.Order_list"
            android:label="@string/title_activity_order_list"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity
            android:name=".Activity.View_rating_review"
            android:label="@string/title_activity_view_rating_review"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity
            android:name=".Activity.mange_stock"
            android:label="@string/title_activity_mange_stock"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity android:name=".Activity.home" />
        <activity android:name=".Activity.Homenavigation" />
        <activity android:name=".Activity.Signup" />
        <activity android:name=".Activity.activity_login" />
        <activity android:name=".Activity.Profile" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>